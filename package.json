{
  "name": "@ogim/metadata",
  "version": "1.0.0",
  "description": "Read & write metadata",
  "main": "src/index.js",
  "preferGlobal": "true",
  "bin": {
    "metadata": "./bin/index.js"
  },
  "dependencies": {
    "atob": "^2.1.2",
    "btoa": "^1.2.1",
    "co": "^4.6.0",
    "co-prompt": "^1.0.0",
    "commander": "^2.19.0",
    "plist": "^3.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-flow-strip-types": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-flow": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "eslint": "^5.9.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-config-react-app": "^3.0.5",
    "eslint-config-with-prettier": "^5.0.0",
    "eslint-plugin-flowtype": "^3.2.0",
    "eslint-plugin-json": "^1.3.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-require-jsdoc-except": "^1.3.0",
    "flow-bin": "^0.86.0",
    "rimraf": "^2.6.2",
    "uglify-js": "^3.4.9"
  },
  "scripts": {
    "start": "node -r @babel/register src/index.js",
    "transpile": "babel -d bin/temp src --no-babelrc --config-file=./.babelrc-es5 --ignore \"src/**/*.debug.js\",\"src/**/*.test.js\"",
    "minifyJS": "uglifyjs --compress --mangle --output ./bin/package.js -- ./bin/temp/index.js",
    "build": "npm run clean &&  npm run transpile && npm run minifyJS && npm run clean",
    "clean": "mkdir bin/temp; rimraf bin/temp/*",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ogim/metadata.git"
  },
