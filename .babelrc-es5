{
  "presets": [["@babel/preset-env",{
    "targets": {
      "node": "8"
    },
    modules: "commonjs",
    useBuiltIns: "usage",
    debug: true,
  }],"@babel/preset-flow"],
  "plugins": [
    "@babel/transform-flow-strip-types",
    "@babel/plugin-proposal-object-rest-spread",
    "dynamic-import-node",
    ["@babel/plugin-transform-runtime",{
        "corejs": false,
        "helpers": true,
        "regenerator": true,
        "useESModules": false
    }]
  ]
}

